



//here getlength is a funcation which will take the i/p string then it will count the element upto the null value 

getlength(char name[]){
count = 0;
for(int i=0; name[i]!='\0'; i++){
     count++;
}
return count;

}

// as the null value occure then the element will stop counting and then the total count will be pass towards the funcation.

int main(){
 char name[20];
 cin>>name;
 cout<<"the length of the given string is"<<getlength(name);
 
 }
